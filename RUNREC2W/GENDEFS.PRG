MODI WINDOW SCREEN TITLE "Recover 2.0 .DEF file generation."
SET SYSMENU OFF
*SET CPDIALOG OFF
USE RecovLst SHARED
SCAN FOR !DELETED() AND DoGenDef
  MODI WINDOW SCREEN TITLE "Generating .DEF file for: " + ALLT(TABLEPATH)
  REPLACE RecovLst.GenDefRes;
    WITH GENDEF(ALLT(TABLEPATH), ALLT(MEMOFILE), ALLT(GENDEFOPTS))
ENDSCAN
QUIT
